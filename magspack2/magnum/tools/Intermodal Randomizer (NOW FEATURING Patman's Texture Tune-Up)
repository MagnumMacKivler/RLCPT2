                holoParent(H,Car)
                if(SkinOverride[Holo:model(),number]!=0){
                    holoSkinEx(H,SkinOverride[Holo:model(),number],CType)
                }else{
                    holoSkinEx(H,randint(1,Holo:getSkinCount() + ExtraSkins[CType,array]:count() - 1),CType)
                }
                holoBodygroup(H,1,1)
                #Bottoms[Iteration,entity] = Holo
